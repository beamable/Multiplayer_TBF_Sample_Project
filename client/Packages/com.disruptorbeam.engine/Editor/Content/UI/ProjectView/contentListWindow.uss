#progress-bar-container {
    flex-direction: row;
    height: 12px;
    position: relative;
    margin-left: 6px;
    margin-right: 6px;
    background-color: #484848;
}

#progress-bar {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    background-color: #4F80F8;
}

ScrollView {
    flex-grow: 1;
}

ScrollView #ContentView.content-view {
    position: absolute;
    left: 0px;
    right: 0px;

}

.filter-text-label {
