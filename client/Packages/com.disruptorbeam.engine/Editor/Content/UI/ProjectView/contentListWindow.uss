#list-root {
    flex-grow: 1;
}
#_windowRoot {
    flex-grow: 5;
    background-color: #272727;
}


#_windowRoot Label#contentListTitle {
    margin-top: 10px;
    margin-left: 15px;
    margin-bottom: 7px;
    height: 30px;
    width: 185px;
    font-size: 18px;
    -unity-font: url("project:///Packages/com.disruptorbeam.engine/Editor/Font/Lato-Semibold.ttf");
    border-width: 0px;
    background-color: rgba(0,0,0,0);
    color: #EFEFEF;
    cursor: pan;
    white-space: normal;
}

#_windowRoot Button#publish{
    height: 25px;
    width: 80px;
    position: absolute;
    top:10px;
    right: 100px;
    background-color: #484848;
    font-size: 10px;
    -unity-font: url("project:///Packages/com.disruptorbeam.engine/Editor/Font/Lato-Semibold.ttf");
    color: #FFFFFF;
    border-radius:8px;
    cursor: pan;
}

#_windowRoot Button#refresh{
    height: 25px;
    width: 80px;
    position: absolute;
    top: 10px;
    right: 15px;
    font-size: 10px;
    -unity-font: url("project:///Packages/com.disruptorbeam.engine/Editor/Font/Lato-Semibold.ttf");
    color: #FFFFFF;
    background-color: #484848;
    border-radius: 8px;
    cursor: pan;
}

#_windowRoot Button#download{
    height: 25px;
    width: 80px;
    position: absolute;
    top: 10px;
    right: 185px;
    font-size: 10px;
    -unity-font: url("project:///Packages/com.disruptorbeam.engine/Editor/Font/Lato-Semibold.ttf");
    color: #FFFFFF;
    background-color: #484848;
    border-radius: 8px;
    cursor: pan;
}

#tip-label-container {
    background-color: #272727;
    padding-top: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    padding-right: 10px;
}

#tip-label-container > #tip-label {
    white-space: normal;
    font-size: 12px;
    color:#BEBEBE;
    -unity-font: url("project:///Packages/com.disruptorbeam.engine/Editor/Font/Lato-Semibold.ttf");
}

